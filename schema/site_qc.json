{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://github.com/FDA-ARGOS/data.argosdb/tree/1.0/schema/site_qc.json",
    "title": "Root",
    "type": "object",
    "required": [
        "fdaargos_seq_feature",
        "genomeic_identifier"
    ],
    "properties": {
        "fdaargos_seq_feature": {
            "$id": "#root/fdaargos_seq_feature",
            "title": "Fdaargos_seq_feature",
            "description": "Identifier for this data object",
            "type": "string",
            "default": "",
            "examples": [
                "ex101"
            ],
            "pattern": "^.*$"
        },
        "genomeic_identifier": {
            "$id": "#root/genomeic_identifier",
            "title": "Genomeic_identifier",
            "description": "identifirer for the source genomic object",
            "type": "string",
            "default": "",
            "examples": [
                "OK394248"
            ],
            "pattern": "^.*$"
        },
        "num_nuc_call": {
            "$id": "#root/num_nuc_call",
            "title": "Num_nuc_call",
            "description": "Number of nucliotides called for this position",
            "type": "integer",
            "examples": [
                10
            ],
            "default": 0
        },
        "percent_nuc_call": {
            "$id": "#root/percent_nuc_call",
            "title": "Percent_nuc_call",
            "description": "Total percentage of nucleotieds at this position that matched this call",
            "type": "number",
            "examples": [
                0.23
            ],
            "default": 0.0
        },
        "genome_assembly_id": {
            "$id": "#root/genome_assembly_id",
            "title": "Genome_assembly_id",
            "description": "The NCBI or ENA assembly id for this sequence",
            "type": "string",
            "default": "",
            "examples": [
                "GCA_013267415.1"
            ],
            "pattern": "^.*$"
        },
        "genomic_coordinates_start": {
            "$id": "#root/genomic_coordinates_start",
            "title": "Genomic_coordinates_start",
            "description": "Start position for this feature in the genome file",
            "type": "integer",
            "examples": [
                238
            ],
            "default": 0
        },
        "genomic_coordinates_end": {
            "$id": "#root/genomic_coordinates_end",
            "title": "Genomic_coordinates_end",
            "description": "End position for this feature in the genome file",
            "type": "integer",
            "examples": [
                21527
            ],
            "default": 0
        },
        "protein_coordinates_start": {
            "$id": "#root/protein_coordinates_start",
            "title": "Protein_coordinates_start",
            "description": "Start position for this feature in the proteome file",
            "type": "integer",
            "examples": [
                79
            ],
            "default": 0
        },
        "protein_coordinates_end": {
            "$id": "#root/protein_coordinates_end",
            "title": "Protein_coordinates_end",
            "description": "End position for this feature in the proteome file",
            "type": "integer",
            "examples": [
                7176
            ],
            "default": 0
        },
        "forward_reverse_disbalance": {
            "$id": "#root/forward_reverse_disbalance",
            "title": "Forward_reverse_disbalance",
            "description": "Forward reverse disbalance",
            "type": "string",
            "default": "",
            "examples": [
                "0.5"
            ],
            "pattern": "^.*$"
        },
        "source": {
            "$id": "#root/source",
            "title": "Source",
            "description": "Source database or resource for this feature",
            "type": "string",
            "default": "",
            "examples": [
                "genbank"
            ],
            "pattern": "^.*$"
        },
        "seq_feature_tag ": {
            "$id": "#root/eq_feature_tag ",
            "title": "Eq_feature_tag ",
            "description": "Tags associated with this feature",
            "type": "string",
            "default": "",
            "examples": [
                "gene"
            ],
            "pattern": "^.*$"
        },
        "seq_feature_value": {
            "$id": "#root/seq_feature_value",
            "title": "Seq_feature_value",
            "description": "Values for the tag associated with this feature",
            "type": "string",
            "default": "",
            "examples": [
                "ORF1ab"
            ],
            "pattern": "^.*$"
        }
    }
}
